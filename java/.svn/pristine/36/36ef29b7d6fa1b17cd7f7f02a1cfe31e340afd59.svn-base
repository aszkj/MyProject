package com.yilidi.o2o.appvo.buyer.product;

import java.util.List;

import com.yilidi.o2o.appvo.AppBaseVO;

/**
 * 商品类别
 * 
 * @author: chenb
 * @date: 2016年5月27日 下午7:26:46
 */
public class ProductClassVO extends AppBaseVO {
	private static final long serialVersionUID = 4971955272559408454L;

	/** 商品分类CODE **/
	private String classCode;
	/** 商品分类名称 **/
	private String className;
	/** 商品分类图片URL **/
	private String classImageUrl;
	/** 商品分类list **/
	private List<ProductClassVO> subClassList;

	public String getClassCode() {
		return classCode;
	}

	public void setClassCode(String classCode) {
		this.classCode = classCode;
	}

	public String getClassName() {
		return className;
	}

	public void setClassName(String className) {
		this.className = className;
	}

	public List<ProductClassVO> getSubClassList() {
		return subClassList;
	}

	public void setSubClassList(List<ProductClassVO> subClassList) {
		this.subClassList = subClassList;
	}

    public String getClassImageUrl() {
        return classImageUrl;
    }

    public void setClassImageUrl(String classImageUrl) {
        this.classImageUrl = classImageUrl;
    }

}
