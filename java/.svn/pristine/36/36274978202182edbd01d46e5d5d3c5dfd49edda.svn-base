package com.yilidi.o2o.appvo.buyer.product;

import com.yilidi.o2o.appvo.AppBaseVO;

/**
 * 广告VO
 * 
 * @author: chenlian
 * @date: 2016年8月26日 下午12:42:52
 */
public class AdvertiseVO extends AppBaseVO {

    private static final long serialVersionUID = 1L;

    /**
     * 广告名称
     */
    private String advertisementName;

    /**
     * 广告跳转链接类型。 0:没有链接 1:商品分类 2:专区页面 3:首页分类专区页面 4:专题 5:活动 6:H5页面 7:商品详情页 8:网站资讯、公告
     */
    private Integer linkType;

    /**
     * 广告关键链接数据
     */
    private String linkData;

    /**
     * 广告图片链接URL
     */
    private String advertisementImageUrl;

    public String getAdvertisementName() {
        return advertisementName;
    }

    public void setAdvertisementName(String advertisementName) {
        this.advertisementName = advertisementName;
    }

    public Integer getLinkType() {
        return linkType;
    }

    public void setLinkType(Integer linkType) {
        this.linkType = linkType;
    }

    public String getLinkData() {
        return linkData;
    }

    public void setLinkData(String linkData) {
        this.linkData = linkData;
    }

    public String getAdvertisementImageUrl() {
        return advertisementImageUrl;
    }

    public void setAdvertisementImageUrl(String advertisementImageUrl) {
        this.advertisementImageUrl = advertisementImageUrl;
    }

}
