package com.yilidi.o2o.common.session.model;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

/**
 * 封装HttpServletRequest、HttpServletResponse，YiLiDiSession的Model类
 * 
 * @author chenl
 *
 */
public class ReqAndRespAndSessionModel {

	private HttpServletRequest request;

	private HttpServletResponse response;

	private YiLiDiSession session;

	public HttpServletRequest getRequest() {
		return request;
	}

	public void setRequest(HttpServletRequest request) {
		this.request = request;
	}

	public HttpServletResponse getResponse() {
		return response;
	}

	public void setResponse(HttpServletResponse response) {
		this.response = response;
	}

	public YiLiDiSession getSession() {
		return session;
	}

	public void setSession(YiLiDiSession session) {
		this.session = session;
	}

}
