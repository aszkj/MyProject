	rel="stylesheet" />
<link type="text/css" href="/static-resource/css/module.css"
	rel="stylesheet" />
<link type="text/css" href="/static-resource/css/system.css"
	rel="stylesheet" />
<link type="text/css" href="/static-resource/css/timepicker.css"
	rel="stylesheet" />
<script type="text/javascript">
	var param = {
		currentPage : 1,
		pageSize : 10,
		pageCount : 0,
		totalCount : 0
	};
	function updatePageInfo() {
		if (param['totalCount'] < 1) {
			$(".main_pagination").hide();
		} else {
			$(".main_pagination").show()
		}
		var html = [];
		var i = 1;
		var j = 5;
		if (param['pageCount'] >= 5) {
			i = (param['currentPage'] + 2) <= param['pageCount'] ? ((param['currentPage'] - 2 >= 1) ? (param['currentPage'] - 2)
					: 1)
					: (param['pageCount'] - 4);
			j = i + 4;
		} else {
			j = param['pageCount'];
		}
		if (param['currentPage'] == 1) {
			html.push('<span class="page_prev">&lt;&lt; 上一页</span>');
		} else {
			html
					.push('<a onclick="goPage('
							+ (param['currentPage'] - 1)
							+ ')" href="javascript:void(0);" class="page_prev">&lt;&lt; 上一页</a>');
		}
		for (; i <= j; i++) {
			if (i == param['currentPage']) {
				html.push('<a href="javascript:void(0);" class="bg_on">');
				html.push(i);
				html.push('</a>');
			} else {
				html.push('<a onclick="goPage(');
				html.push(i);
				html.push(');" href="javascript:void(0);" >');
				html.push(i);
				html.push('</a>');
			}
		}
		if (param['currentPage'] == param['pageCount']) {
			html.push('<span class="page_next">下一页 &gt;&gt;</span>');
		} else {
			html
					.push('<a onclick="goPage('
							+ (param['currentPage'] + 1)
							+ ')" href="javascript:void(0);" class="page_next">下一页 &gt;&gt;</a>');
		}
		$("#pageItem").html(html.join(''));
		$("#pageCount").html(param['pageCount']);
		$("#totalCount").html(param['totalCount']);
	}
	function goPage(pageNo) {
		var pattern = /\d+/;
		if (!pattern.test(parseInt(pageNo)))
			return;
		if (parseInt(pageNo) < 1) {
			if (param['currentPage'] != 1) {
				showBox('提示信息', '您输入的页码超出正常范围', 'alert');
			} else {
				showBox('提示信息', '当前已经是第一页', 'alert');
			}
			return;
		}
		if (pageNo > param['pageCount']) {
			if (param['currentPage'] != param['pageCount']) {
				showBox('提示信息', '您输入的页码超出正常范围', 'alert');
			} else {
				showBox('提示信息', '当前已经是最后一页', 'alert');
			}
			return;
		}
		dataSubmit(pageNo);
	}
	function dataSubmit(pageNo) {
		var pattern = /\d+/;
		if (pattern.test(parseInt(pageNo))) {
			param.currentPage = pageNo;
		}
		$('#start').val(param.currentPage);
		$('#pageSize').val(param.pageSize);
		var formJsonStr = JSON.stringify(iTsai.form.serialize($('#dataForm')));
		$
				.ajax({
					type : 'post',
					contentType : 'application/json',
					data : formJsonStr,
					dataType : 'json',
					error : function() {
						showBox('提示信息', '提交数据失败，请稍后再试', 'alert');
					},
					url : TERMINAL_URL + OPERATION_SYSTEM_IDENTITY
							+ '/usersharerecord/search',
					beforeSubmit : function() {
					},
					success : function(data) {
						//响应返回的业务处理  
						if (data.msgCode == 1) {
							//特殊处理 尖括号
							var temp = JSON.stringify(data);
							temp = encodeString(temp);
							data = JSON.parse(temp);

							var html = '';
							var dataList = data.entity.resultList;
							var id = getUrlParam('id');
							if (null != dataList && dataList.length > 0) {
								for (var i = 0, j = dataList.length; i < j; i++) {
									html += '<tr><td>';
									html += ((param.currentPage - 1)
											* (param.pageSize) + i + 1);
									html += '</td><td>';
									html += dataList[i].shareUserName;
									html += '</td><td>';
									html += dataList[i].prevWeekCount || 0;
									html += '</td><td>';
									html += dataList[i].curWeekCount || 0;
									html += '</td><td>';
									html += dataList[i].accumulativeCount || 0;
									html += '</td><td>';
									html += toCenti(
											dataList[i].accumulativeIncome, 2);
									html += '</td><td>';
									html += dataList[i].accumulativePoints || 0;
									html += '</td><td>';
									html += dataList[i].accumulativeCoupon || 0;
									html += '</td><td>';
									html += '<permission code="FUNCTION_DETAIL_USERSHARE_MANAGEMENT">';
									html += '<a href="javascript:void(0)" onclick="detail('
											+ dataList[i].shareUserId
											+ ')" class="ablue">查看明细</a>&nbsp;&nbsp;&nbsp;';
									html += '</permission>';
									html += '</td></tr>';
								}
							} else {
								html += '<tr><td colspan="9">暂无数据</td></tr>';
							}
							$("#tbody").html(html);
							param.currentPage = data.entity.currentPage;
							param.pageCount = data.entity.pageCount;
							param.totalCount = data.entity.recordCount;
							updatePageInfo();
							disposeFunctionPermissions();
						} else {
							showBox('提示信息', data.msg, 'alert');
						}
					}
				});
	}
	function detail(id) {
		locationHref('/share/usershare-detail.html?shareUserId=' + id);
	}
	/*获取返回页面的上次查询条件*/
	function setParamValue() {
		$('#shareUserName').val(getUrlParam('shareUserName'));
		if ("" != getUrlParam('pageSize') && "" != getUrlParam('start')) {
			param.pageSize = getUrlParam('pageSize');
			param.currentPage = getUrlParam('start');
		}
	}
	function getParamValue() {
		var passParam = [];
		var shareUserName = $.trim($("#shareUserName").val());
		var start = $.trim($("#start").val());
		var pageSize = $.trim($("#pageSize").val());

		passParam.push('shareUserName=' + shareUserName);
		passParam.push('start=' + start);
		passParam.push('pageSize=' + pageSize);
		return passParam.join('&');
	}
	/*加载时间控件*/
	function loadTimeInput() {
		$('#strStartTime').datepicker({
			dateFormat : 'yy-mm-dd',
			constrainInput : true
		});
		$('#strEndTime').datepicker({
			dateFormat : 'yy-mm-dd',
			constrainInput : true
		});
	}
	$(document).ready(function(e) {
		loadHtml("headerId", "/include/header.html");
		loadHtml("left_menu_content", "/include/leftmenu.html");
		loadHtml("footerId", "/include/footer.html");
		//loadTimeInput();
		dataSubmit();
	});
</script>
</head>

<body id="bodyId">
	<div id="headerId"></div>
	<div class="bread">
		<a href="/">首页</a> &gt; 分享管理 &gt; <span>用户分享记录</span>
	</div>
	<div class="main">
		<div class="clearfix">
			<div class="content_box">
				<div class="content">
					<div class="page_title">用户分享记录</div>
					<div class="complaint_main">
						<form method="get" id="dataForm">
							<div class="main_search">
								<dl>
									<dt>邀请人手机号:</dt>
									<dt>
										<input type="text" class="txt grid-12" id="shareUserName"
											name="shareUserName" />
									</dd>
								</dl>
								<!-- 
								<dl>
									<dt>时间：</dt>
									<dd>
										<input type="text" class="txt grid-11" id="strStartTime"
											name="strStartTime" />
									</dd>
									<dt class="mlr5">至</dt>
									<dd>
										<input type="text" class="txt grid-11" id="strEndTime"
											name="strEndTime" />
									</dd>
								</dl>
								-->
								<input type="hidden" id="start" name="start" /> <input
									type="hidden" id="pageSize" name="pageSize" />
								<p class="main_search_btn">
									<permission code="FUNCTION_SEARCH_USERSHARE_MANAGEMENT">
									<input type="button" value="查找" class="btn_sure" id="search"
										onClick="dataSubmit(1)" /> </permission>
								</p>
							</div>

							<!--搜索结束-->
						</form>
						<div class="order_index" style="overflow: auto;">
							<table class="simple_table" cellpadding="0" cellspacing="0"
								width="100%">
								<thead>
									<tr>
										<th width="40">序号</th>
										<th width="160">邀请人手机号</th>
										<th width="80">上周邀请人数</th>
										<th width="120">本周邀请人数</th>
										<th width="120">累计邀请人数</th>
										<th width="120">累计收益</th>
										<th width="120">累计积分</th>
										<th width="120">累计优惠劵</th>
										<th width="100">操作</th>
									</tr>
								</thead>
								<tbody id="tbody"></tbody>
							</table>
						</div>
						<dl class="main_pagination">
							<dd class="pagination">
								<span id="pageItem"> <span class="page_prev">&lt;&lt;
										上一页</span> <a class="bg_on" href="javascript:void(0);">1</a> <a
									href="javascript:void(0);" onClick="goPage(2);">2</a> <a
									class="page_next" href="javascript:void(0);"
									onClick="goPage(2)">下一页 &gt;&gt;</a>
								</span> <span class="page_no">共<span id="totalCount">0</span>条
								</span> <span class="page_no">共<span id="pageCount">2</span>页 到第
									<input type="text"
									onKeyUp="this.value=this.value.replace(/^0+|\D/g,'')"
									class="page_txt" id="pageObj">页 <input type="button"
									onClick="goPage($.trim($('#pageObj').val()));" class="page_btn"
									value="确定"></span>
							</dd>
						</dl>
					</div>
				</div>
			</div>
			<div class="menu">
				<div class="aside">
					<h2 class="aside_title">一里递运营中心</h2>
					<div id="left_menu_content"></div>
				</div>
			</div>
		</div>
	</div>
	<div id="footerId"></div>
</body>
