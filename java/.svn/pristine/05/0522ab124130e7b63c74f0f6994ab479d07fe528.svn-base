package com.yilidi.o2o.product.service.dto.query;

import com.yilidi.o2o.core.model.BaseQueryDto;

/**
 * 功能描述：场次单查询条件封装类 <br/>
 * 作者：chenb <br/>
 * 
 * BugID: <br/>
 * 修改内容： <br/>
 */
public class SecKillSceneQueryDto extends BaseQueryDto {

    private static final long serialVersionUID = 3072124739137145053L;
    /**
     * 场次名称
     */
    private String sceneName;
    /**
     * 场次状态编码
     */
    private String statusCode;
    /**
     * 重复类型
     */
    private String repeatType;
    /**
     * 场次抢购开始时间字符串
     */
    private String strBeginStartTime;
    /**
     * 场次抢购结束时间字符串
     */
    private String strEndStartTime;
    /**
     * 秒杀商品ID
     */
    private Integer secKillProductId;

    public String getSceneName() {
        return sceneName;
    }

    public void setSceneName(String sceneName) {
        this.sceneName = sceneName;
    }

    public String getStatusCode() {
        return statusCode;
    }

    public void setStatusCode(String statusCode) {
        this.statusCode = statusCode;
    }

    public String getRepeatType() {
        return repeatType;
    }

    public void setRepeatType(String repeatType) {
        this.repeatType = repeatType;
    }

    public String getStrBeginStartTime() {
        return strBeginStartTime;
    }

    public void setStrBeginStartTime(String strBeginStartTime) {
        this.strBeginStartTime = strBeginStartTime;
    }

    public String getStrEndStartTime() {
        return strEndStartTime;
    }

    public void setStrEndStartTime(String strEndStartTime) {
        this.strEndStartTime = strEndStartTime;
    }

    public Integer getSecKillProductId() {
        return secKillProductId;
    }

    public void setSecKillProductId(Integer secKillProductId) {
        this.secKillProductId = secKillProductId;
    }

}