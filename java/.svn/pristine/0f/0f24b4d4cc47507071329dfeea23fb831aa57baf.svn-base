package com.yilidi.o2o.system.jms.model.normal;

import java.util.Date;

import com.yilidi.o2o.core.model.JmsMessageModel;

/**
 * 用户分享奖励消息
 * 
 * @author: chenbin
 * @date: 2016年5月26日 下午4:24:57
 */
public class UserShareAwardMessageModel extends JmsMessageModel {

    private static final long serialVersionUID = -1433991823427021833L;
    /**
     * 用户ID
     */
    private Integer userId;
    /**
     * 订单编号
     */
    private String orderNo;
    /**
     * 操作时间
     */
    private Date nowTime;
    /**
     * 奖励条件类型
     */
    private String awardConditionType;

    public Integer getUserId() {
        return userId;
    }

    public void setUserId(Integer userId) {
        this.userId = userId;
    }

    public String getOrderNo() {
        return orderNo;
    }

    public void setOrderNo(String orderNo) {
        this.orderNo = orderNo;
    }

    public Date getNowTime() {
        return nowTime;
    }

    public void setNowTime(Date nowTime) {
        this.nowTime = nowTime;
    }

    public String getAwardConditionType() {
        return awardConditionType;
    }

    public void setAwardConditionType(String awardConditionType) {
        this.awardConditionType = awardConditionType;
    }

}
