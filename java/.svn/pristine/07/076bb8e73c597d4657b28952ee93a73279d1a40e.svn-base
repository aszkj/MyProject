package com.yilidi.o2o.order.service.dto;

import com.yilidi.o2o.core.model.BaseDto;

/**
 * 同步调整购物车数量
 * 
 * @author: chenb
 * @date: 2016年6月3日 上午10:59:52
 */
public class AsycnChangeCartDto extends BaseDto {

    private static final long serialVersionUID = -1590224188999658798L;

    /**
     * 当前购物车总数量
     */
    private Integer cartCount;
    /**
     * 当前购物车总金额
     */
    private Long cartAmount;

    public Integer getCartCount() {
        return cartCount;
    }

    public void setCartCount(Integer cartCount) {
        this.cartCount = cartCount;
    }

    public Long getCartAmount() {
        return cartAmount;
    }

    public void setCartAmount(Long cartAmount) {
        this.cartAmount = cartAmount;
    }

}
