package com.yilidi.o2o.appvo.buyer.product;

import com.yilidi.o2o.appvo.AppBaseVO;

/**
 * 轮播广告图
 * 
 * @Description: 轮播广告图
 * @author: chenb
 * @date: 2016年5月27日 上午9:40:34
 */
public class AdvertisementVO extends AppBaseVO {

    private static final long serialVersionUID = 1L;
    /**
     * 链接类型。(暂未定好类型种类) <br>
     * 0:没有链接 1:商品分类 2:H5页面 3:首页分类专区页面（废弃） 4:专题 5:活动 6:商品详情页 7:网站资讯、公告
     **/
    private Integer linkType;
    /**
     * 广告关键链接数据：（以key=value形式传递，多个以&连接） 0： 1：classCode 2：zoneType值见3.35接口请求参数值 3：zoneCode（废弃） 4：themeType 5：activityType
     * 6：barCode 7：linkUrl
     **/
    private String linkData;
    /** 图片链接地址 **/
    private String imageUrl;
    /** 标题名称 **/
    private String titleName;

    public Integer getLinkType() {
        return linkType;
    }

    public void setLinkType(Integer linkType) {
        this.linkType = linkType;
    }

    public String getLinkData() {
        return linkData;
    }

    public void setLinkData(String linkData) {
        this.linkData = linkData;
    }

    public String getImageUrl() {
        return imageUrl;
    }

    public void setImageUrl(String imageUrl) {
        this.imageUrl = imageUrl;
    }

    public String getTitleName() {
        return titleName;
    }

    public void setTitleName(String titleName) {
        this.titleName = titleName;
    }

}
