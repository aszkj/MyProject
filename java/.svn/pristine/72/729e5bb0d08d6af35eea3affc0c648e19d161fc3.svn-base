<script type="text/javascript">
	var UA = navigator.userAgent.toLowerCase();
	var isPhone = RegExp("iphone").test(UA);
	var isAndroid = RegExp("android").test(UA);
	if (isPhone)
		document.getElementById('rule-ios').style.display = 'block';
	$(".m-vip-btn").click(function() {
		var productId=$(this).attr("data-product");
		if (isAndroid)
			window.cartObject.addVipToCart(productId);
		if (isPhone)
			window.webkit.messageHandlers.addVipToCart.postMessage(productId);
	});
</script>
