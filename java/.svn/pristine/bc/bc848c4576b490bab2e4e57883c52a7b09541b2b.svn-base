</body>
<script type="text/javascript">
	var UA = navigator.userAgent.toLowerCase();
	var isPhone = RegExp("iphone").test(UA);
	var isAndroid = RegExp("android").test(UA);
	if (isPhone){
		document.getElementById('rule-ios').style.display = 'block';
		$(".r-act-con").css("height","6.46rem");
	}
	$(".reg-btn").click(function() {
		if (isAndroid)
			window.cartObject.addFenToCart();
		if (isPhone)
			window.webkit.messageHandlers.addFenToCart.postMessage('1');
	});
	$(".b-btn input").click(function() {
		if (isAndroid)
			window.cartObject.toVipHtml();
		if (isPhone)
			window.webkit.messageHandlers.toVipHtml.postMessage('1');
	});
</script>
</html>
