 */
public class ShareRule extends BaseModel {

    private static final long serialVersionUID = 1629062794913943234L;

    /**
     * ID，主键自增
     */
    private Integer id;
    /**
     * 分享规则名称
     */
    private String shareRuleName;
    /**
     * 有效开始时间
     */
