    /**
     * 被邀请人限制条件,关联系统域系统字典S_SYSTEM_DICT中的DICTCODE字段(DICTTYPE=SHARERULEINVITEDCONDITIONTYPE)
     */
    private String invitedConditionType;
    /**
     * 被邀请人条件设置商品ID,多个以逗号隔开
     */
    private String invitedUseProduct;
