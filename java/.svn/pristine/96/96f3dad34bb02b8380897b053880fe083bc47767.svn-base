        this.invitedAwardType = invitedAwardType;
    }


    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getH5DrawUrl() {
        return h5DrawUrl;
    }

    public void setH5DrawUrl(String h5DrawUrl) {
