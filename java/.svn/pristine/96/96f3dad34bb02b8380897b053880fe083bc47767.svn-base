package com.yilidi.o2o.user.model;

import java.util.Date;

import com.yilidi.o2o.core.model.BaseModel;

/**
 * 分享规则表，映射用户域表YiLiDiUserCenter.U_SHARE_RULE
 * 
 * @author: chenb
