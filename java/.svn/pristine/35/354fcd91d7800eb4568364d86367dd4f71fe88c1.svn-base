package com.yilidi.o2o.order.service.dto;

import java.util.List;

import com.yilidi.o2o.core.model.BaseDto;

/**
 * 调拨单保存DTO
 * 
 * @author simpson
 * 
 */
public class FlittingOrderSaveDto extends BaseDto {

    private static final long serialVersionUID = 8962131752116503778L;
    /**
     * 调出店铺ID
     */
    private Integer srcStoreId;
    /**
     * 调入店铺ID
     */
    private Integer destStoreId;
    /**
     * 调货单备注
     */
    private String flittingNote;
    /**
     * 操作用户ID
     */
    private Integer operateUserId;
    /**
     * 调拨商品及数量
     */
    private List<FlittingOrderSaveItemDto> flittingProductList;

    public Integer getSrcStoreId() {
        return srcStoreId;
    }

    public void setSrcStoreId(Integer srcStoreId) {
        this.srcStoreId = srcStoreId;
    }

    public Integer getDestStoreId() {
        return destStoreId;
    }

    public void setDestStoreId(Integer destStoreId) {
        this.destStoreId = destStoreId;
    }

    public String getFlittingNote() {
        return flittingNote;
    }

    public void setFlittingNote(String flittingNote) {
        this.flittingNote = flittingNote;
    }

    public Integer getOperateUserId() {
        return operateUserId;
    }

    public void setOperateUserId(Integer operateUserId) {
        this.operateUserId = operateUserId;
    }

    public List<FlittingOrderSaveItemDto> getFlittingProductList() {
        return flittingProductList;
    }

    public void setFlittingProductList(List<FlittingOrderSaveItemDto> flittingProductList) {
        this.flittingProductList = flittingProductList;
    }

}
