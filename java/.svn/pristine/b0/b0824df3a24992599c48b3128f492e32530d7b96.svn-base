package com.yilidi.o2o.order.model.query;

import com.yilidi.o2o.core.model.BaseQuery;

/**
 * 功能描述：采购单收化地址查询条件封装类 <br/>
 * 作者：chenb <br/>
 * 
 * BugID: <br/>
 * 修改内容： <br/>
 */
public class PurchaseOrderAddressQuery extends BaseQuery {
    private static final long serialVersionUID = 7681492515562233746L;

    /**
     * 采购单号
     */
    private String purchaseOrderNo;
    /**
     * 收货人姓名
     */
    private String userName;
    /**
     * 收货人电话号码
     */
    private String phoneNo;

    public String getPurchaseOrderNo() {
        return purchaseOrderNo;
    }

    public void setPurchaseOrderNo(String purchaseOrderNo) {
        this.purchaseOrderNo = purchaseOrderNo;
    }

    public String getUserName() {
        return userName;
    }

    public void setUserName(String userName) {
        this.userName = userName;
    }

    public String getPhoneNo() {
        return phoneNo;
    }

    public void setPhoneNo(String phoneNo) {
        this.phoneNo = phoneNo;
    }

}