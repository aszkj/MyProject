.writeBtn3,.writeBtn3:hover{color: #fe9100;}
.btns,.yellowBtn,.greenBtn,.btns:hover,.yellowBtn:hover,.greenBtn:hover,.blueBtn,.blueBtn:hover{color: #fff;}
