.color4,.color4:hover{color: #fe9100;}/*黄色*/
.color5,.color5:hover{color: #75c60d;}/*绿色*/
a:hover,a:focus{text-decoration: none;outline: 0}
.mini-btns{padding-top: 0.25rem!important;padding-bottom: 0.25rem!important;width: 4rem;}
.btns,.yellowBtn,.blueBtn{padding: .5rem .5rem;border-radius: 5px;text-align: center;font-size: .6rem;display: block;overflow: hidden;}
.writeBtn,.writeBtn2,.writeBtn3,.greenBtn{padding: .5rem .5rem;border-radius: 5px;text-align: center;font-size: .6rem;overflow: hidden;background-color: #fff;display: block;}
.btns{background-color: #fe0000;}
.yellowBtn{background-color: #fe9100;}
.greenBtn{background-color: #35b200;}
