#opw-header,#JS_searchArea .m-serach-all{position: absolute;top: 0;left: 0;width: 100%;}
#opw-header a:hover,#opw-header a:focus,#opw-header a:visited,#opw-header a:link{color: #fff;}
#opw-header,#category-header{background-color: #fe0000;width: 100%;color: #fff;height: 42px;
display: -webkit-box;  display: -webkit-flex;  display: -ms-flexbox;  display: flex;  -webkit-flex-wrap: wrap;  -ms-flex-wrap: wrap;  flex-wrap: wrap;}
.m-search{padding: 5px 0;}
.cell{padding: 10px 0;display: block;color: #fff;-webkit-box-flex: 1;  -webkit-flex: 1;  -ms-flex: 1;  flex: 1; font-size: 16px;  width: 0;  max-width: 100%;  -webkit-flex-basis: 0;  -ms-flex-preferred-size: 0;  flex-basis: 0;}
.h_tab{padding:10px;font-size:14px;color: #fff;-webkit-box-flex: 0;  -webkit-flex: none;  -ms-flex: none;  flex: none;  width: 80px;display: block;}
.h_tab_refer{width: 4.3rem;}
#opw-header a:first-child{padding-right: 0;}
#opw-header a:last-child{padding-left: 0;}
#c-foot{position: absolute;bottom:0; left:0;width:100%;background-color: #fff;border-top: 1px solid #cdcdcd;z-index: 1000;}
.footer,.order-submit{width:100%;display: -webkit-box;  display: -webkit-flex;  display: -ms-flexbox;  display: flex;  -webkit-flex-wrap: wrap;  -ms-flex-wrap: wrap;  flex-wrap: wrap;}
.footer a{display: block;font-size: 0.5rem;text-align: center;padding: .25rem 0;-webkit-box-flex:1; -webkit-flex: 1;  -ms-flex: 1;  flex: 1;}
.footer a span{display: block;margin-top: .15rem;color: #505661;}
.footer a.on span{color:#ff0000;}
.footer a.on .i_home{background:url(../img/icon/iconfont-homefill.png) no-repeat;background-size:contain;}
.footer a.on .i_cart{background:url(../img/icon/iconfont-cart-hover.png) no-repeat;background-size:contain;}
.footer a.on .i_order{background:url(../img/icon/iconfont-form-hover.png) no-repeat;background-size:contain;}
.footer a.on .i_user{background:url(../img/icon/iconfont-wo-hover.png) no-repeat;background-size:contain;}
.footer em{border-radius:100%;color: #fff;background-color: #ff0000;min-width: 0.7rem;height: 0.7rem;display: block;position: absolute;top: 0;right:1rem;text-align: center;line-height: 0.7rem;font-size: .45rem}
#okcart{position: relative;}
.foot_cart{background-color: #f3f3f3;display: -moz-box;  display: -webkit-box;  display: box; }
.foot_cart .cart_txt {-moz-box-flex: 1;  -webkit-box-flex: 1;  box-flex: 1;padding-left: .5rem;width:100%;border-top:1px solid #dcdcdc;}
.foot_cart .cart_txt .cart_txt_num{display: block;border-radius: 100%;background-color: #dd0000;width: 1.5rem;height: 1.5rem;margin-top: 0.25rem}
.foot_cart .cart_txt .cart_txt_num .icon{position: absolute;top:50%;margin-top: -0.5rem;left: 50%;margin-left: -0.5rem;}
.foot_cart .cart_txt .num{display: block;  min-width: 0.7rem;height: 0.7rem;line-height: 0.7rem;color: #ff0000;background-color: #fff;border:1px solid #ff0000;font-size: .5rem;text-align: center;border-radius: 100%;position: absolute;top:-5px;right: -5px;}
.foot_cart .cart_txt .txt{padding-left: 0.5rem;padding-top: 0.25rem;}
.foot_cart .cart_btn{-webkit-box-flex: 0;  -webkit-flex: none;  -ms-flex: none;  flex: none; width: 4.5rem;display: block;background-color: #fdd100;line-height:2rem;text-align: center;}

.banner{ width:16rem; height:6.375rem; position:relative; overflow:hidden;}
.banner .hd{ width:100%;height: 5px; position:absolute; z-index:1; bottom:0; }
.banner .hd ul{ overflow:hidden;background:#334e5f; display: -moz-box;  display: -webkit-box;  display: box;  height: 5px;}
.banner .hd ul li{-moz-box-flex: 1;  -webkit-box-flex: 1;  box-flex: 1;text-indent: 1000rem;}
.banner .hd ul li.on{ background:#fe0000; }
.banner .bd{ position:relative; z-index:0; }
.banner .bd li img{ width:100%;}

.category_item{background-color: #f7f8f8;display: none;top: 0;height: 100%;}
.category_item .opw-category{border-bottom:none;}
.opw-category {border-bottom: 1px solid #dcdcdc;display: table;background-color: #f7f8f8;padding-bottom: 0.5rem;}
.opw-category a{width: 3.2rem;display:inline-block;margin:0.5rem 0 0 0;text-align: center;}
.opw-category span{width: 2.05rem;height: 2.05rem;display: inline-block;}

.opw-act{border-top: 1px solid #ff0000;border-bottom: 1px solid #dcdcdc;}
.opw-act-alink{display: block;display: -webkit-box;  display: -webkit-flex;  display: -ms-flexbox;  display: flex;  -webkit-flex-wrap: wrap;  -ms-flex-wrap: wrap;  flex-wrap: wrap;}
.opw-act-txt{-webkit-box-flex: 1;  -webkit-flex: 1;  -ms-flex: 1;  flex: 1;width:6.4rem;  max-width: 100%; background-color: #fff;  -webkit-flex-basis: 0;  -ms-flex-preferred-size: 0;  flex-basis: 0;}
.opw-act-txt img{width: 5.175rem;height: 3.35rem;}
.opw-act-shop{border-left: 1px solid #eee;-webkit-box-flex: 0;  -webkit-flex: none;  -ms-flex: none;  flex: none;  width: 3.175rem;}

.buy_num{border-radius: 0.25rem;height:1.43rem;border:1px solid #eee;background-color: #f9f9f9;padding: 0 0.3rem;position: relative;
display: -webkit-box;  display: -webkit-flex;  display: -ms-flexbox;  display: flex;  -webkit-flex-wrap: wrap;  -ms-flex-wrap: wrap;  flex-wrap: wrap;}
.buy_num .price{-webkit-box-flex: 1;  -webkit-flex: 1;  -ms-flex: 1;  flex: 1;font-size: 0.5rem;display: block;overflow: hidden;line-height: 1.2rem;}
.buy_num .price big{font-size: 0.7rem;}
.buy_num a.buy_add,.buy_num a.buy_minus{width:1rem;display: block;line-height: 1.1rem;-webkit-box-flex: 0;  -webkit-flex: none;  -ms-flex: none;  flex: none;}
.buy_num a.buy_minus{text-align: left;}
.buy_num a.buy_add{text-align: right;}
.buy_num input{text-align: center;width: 100%;display: block;color: #ff0000;line-height: 1rem;background-color: transparent;
-webkit-box-flex: 1;  -webkit-flex: 1;  -ms-flex: 1;  flex: 1;}
.c-title{padding: 0.35rem 0.5rem;border-top: 1px solid #dcdcdc;border-bottom: 1px solid #dcdcdc;display: block;line-height: 0.8rem;}
.c-title2{position: relative;text-align: center;width: 100%;height: 1.5rem;}
.c-title2:after{content: "";position: absolute;width: 100%;height:1px ;border-bottom: 1px dotted #dcdcdc;left: 0;top: 50%;margin-top: -0.125rem;}
.c-title2 span{background-color: #f5f5f5;display: block;padding: 0 0.5rem;width: 6rem;position: absolute;top:0.25rem;z-index: 1;left: 50%;margin-left: -3rem;color: #8b8b8b;}
.opw-store{margin-bottom: 0.5rem;background-color: #fff;}
.opw-store-item{background-color: #fff;overflow: hidden;}
.opw-store-item li{float: left;padding: 0.4rem;width: 5.35rem;height:9.8rem;border-bottom:1px solid #dcdcdc;}
.opw-store-item li:nth-child(3n-1),.opw-store-item .list-view div:nth-child(3n-1) li{border-left:1px solid #dcdcdc;border-right: 1px solid #dcdcdc;width: 5.3rem;}
.opw-store-item li a.goods_pic{display: block;position: relative;margin-bottom: 0.2rem;}
.opw-store-item li a.goods_pic img{width:4.55rem;margin-bottom: 0.25rem;}
.opw-store-item li a.goods_pic p{line-height: 0.8rem;height: .8rem;overflow: hidden;}
.opw-store-item li a.goods_pic p.color1{margin-bottom: .25rem;}
.opw-store-item li a.goods_pic p strong{font-size: 0.6rem;font-weight: normal;}
.opw-store-item li a.goods_pic span.s-price{position: absolute;line-height:1rem;color:#fff;padding:0 0.2rem;bottom: 2rem;left: 0;background-color: #ff0000;border-radius: .25rem;display: inline-block;}
.opw-store-item li.more a{display: block;margin: 3rem auto;text-align: center;}

.oth_store .oth_store_item{background-color: #fff;border-top: 1px solid #dcdcdc;padding: 0 0.5rem;border-bottom: 1px solid #dcdcdc;color: #8b8b8b;}
.oth_store .oth_store_item a{display: block;padding: 0.35rem 0;  line-height: 0.8rem;border-bottom: 1px solid #dcdcdc;color: #8b8b8b;}
.oth_store .oth_store_item a:last-child{border-bottom:0;}
.oth_store .oth_store_item span{color: #8b8b8b;}
.fly_img{ display: block;width: 18px;height: 18px;line-height: 18px;border-radius: 18px;position: fixed;font-size:12px;z-index: 9999;background-color: red;color: #fff;text-align: center;}

.pagination{margin:20px 20px;}
.pagination li{  padding: 0.2rem; width: auto; height: auto; border-bottom:0;}
.pagination li:nth-child(3n-1){border:0;width: auto}
.table{margin-bottom: 0}

.onlywx{top:0;left:0;right:0;bottom:0;width:100%;height:100%;position:fixed;background: rgba(0,0,0,.6);z-index:1001;display:none;}
.onlyzs{width:13rem;left:50%;margin-left:-6.5rem;display:none;bottom:25%;position:fixed;z-index:1002;}
