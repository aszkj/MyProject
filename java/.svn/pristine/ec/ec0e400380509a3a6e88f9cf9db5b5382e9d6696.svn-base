.sq_img{
	position: relative;
}
.sq_img:before{
  content: '';
  display: inline-block;
  width: 2rem;
  height: 2rem;
  background: url(../images/sq.png)no-repeat;
  background-size: 100% 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 15;
}
.sq_img:after{
	content: '';
	display: inline-block;
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	background: #CCCCCC;
	opacity: .5;
	z-index: 10;
}
