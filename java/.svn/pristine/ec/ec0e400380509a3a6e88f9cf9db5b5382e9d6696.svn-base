.onlyzs .close_btn{border-radius: 1rem;height: 1rem;width: 1rem;color: #fff;text-align: center;line-height: 1rem;display: inline-block;position: absolute;top: -10px;right: -10px;background-color: red;}
.onlyzs .close_btn2{border-radius: 1rem;height: 1.5rem;width: 1.5rem;color: #fff;text-align: center;line-height: 1.5rem;font-size:.9rem;display: inline-block;position: absolute;top: 10px;right: 0px;background-color: red;}
.onlyzs .onlyzs_con{background: url(../images/giftdialog.png) no-repeat ;background-size: 100%;width:13rem;height:14.15rem; }
.getGift-btn{position: absolute;bottom: 1.1rem;left: 3.45rem;width: 8rem;height: 1.625rem;}
