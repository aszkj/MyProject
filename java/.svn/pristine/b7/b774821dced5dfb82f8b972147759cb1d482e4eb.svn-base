				$("#invited").hide();
			}
		});
		$("#inviterAwardType").change(function() {
			var awardTypeValue = $(this).val();
			if ("SHARERULEAWARDTYPE_COUPON" == awardTypeValue) {
				$(".inviterAwardType." + awardTypeValue).html(createInviterCouponInfoHtml());
