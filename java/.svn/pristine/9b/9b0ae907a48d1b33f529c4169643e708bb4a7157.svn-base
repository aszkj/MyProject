package com.yilidi.o2o.appvo.buyer.user;

import com.yilidi.o2o.appvo.AppBaseVO;

/**
 * 我的邀请信息总汇
 * 
 * @author: chenb
 * @date: 2016年5月27日 上午11:41:15
 */
public class MyInviteSummaryInfoVO extends AppBaseVO {

    private static final long serialVersionUID = 6240170510173960824L;
    /**
     * 邀请总人数
     */
    private Integer inviteCount;
    /**
     * 邀请累计获取奖励金额(单位：厘)
     */
    private Long inviteRewardAmount;

    public Integer getInviteCount() {
        return inviteCount;
    }

    public void setInviteCount(Integer inviteCount) {
        this.inviteCount = inviteCount;
    }

    public Long getInviteRewardAmount() {
        return inviteRewardAmount;
    }

    public void setInviteRewardAmount(Long inviteRewardAmount) {
        this.inviteRewardAmount = inviteRewardAmount;
    }

}
