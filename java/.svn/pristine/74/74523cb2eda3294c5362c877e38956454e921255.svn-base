.douboxa{
  width: 100%;
  height: auto;
  overflow: hidden;
  border-radius: 5px;
  margin-top: 5px;
}
.left-pa{
  float: left;
  width: 60%;
  position: relative;
}
.left-pa img{
  width: 100%;
}
.right-da{
  width: 40%;
  height: 100%;
  float: right;
  position: relative;
  font-family: '微软雅黑';
}
.right-pa{
  padding-bottom: 1.2rem;
}
.right-pa img{
  width: 100%;
}
.btn_buy{
  display: inline-block;
  width: 4rem;
  height: 1.2rem;
  background: #fe0000;
  text-align: center;
  color: #fff;
  text-align: center;
  line-height: 1.2rem;
  font-size: .7rem;
  position: absolute;
  bottom: 0;
  left: 50%;
  margin-left: -2rem;
  border-radius: 3px;
}
.douboxb{
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: 5px;
}
.titpa{
  width: 100%;
  height: 1.5rem;
  line-height: 1.5rem;
  margin-bottom: 8px;
  background: url(../images/titbg.jpg) no-repeat center center;
  background-size: 100% 100%;
  text-align: center;
  color: #f9ff13;
  font-size: .7rem;
}
.titpa img{
  width: 100%;
}
.douulb{
  width: 100%;
  height: auto;
  overflow: hidden;
}
.douulb li{
  width: 48%;
  float: left;
  background: #fff;
  padding-bottom: 5px;
  margin-top: 5px;
  position: relative;
}
.douulb li:nth-of-type(2n){
  margin-left: .3rem;
}
.box-px{
  padding: .5rem 0;
}
.box-px img{
  width: 80%;
  height: auto;
}
.box-infoa{
  height: 1.6rem;
  overflow: hidden;
}
.box-infoa,.box-infob{
  font-size: .5rem;
}
.box-infob{
  color: #a5a5a5;
}
.box-infoc{
  font-size: .7rem;
  color: #fe0000;
  font-weight: bolder;
  background: #f9f9f9;
  border:1px solid #e5e5e5;
}
.box-infoc span{
  font-size: .5rem;
}
.addico{
  display: inline-block;
  font-weight: normal;
  background:url(../images/addico.png) no-repeat;
  background-size: 100% 100%;
  width: .7rem;
  height: .7rem;
  text-align: center;
  margin-right: 5px;
  float: right;
  margin-top: 5px;
}
.removeico{
  display: inline-block;
  font-weight: normal;
  background:url(../images/removeico.png) no-repeat;
  background-size: 100% 100%;
  width: .7rem;
  height: .7rem;
  text-align: center;
  margin-right: 5px;
  float: left;
  margin-top: 5px;
}
.left-pa:before{
  content: '';
  display: inline-block;
  width: 1.8rem;
  height: 1.8rem;
  position: absolute;
  background: url(../images/ico_hot.png) no-repeat;
  background-size: 100% 100%;
  left:0;
  top:0; 
  z-index: 100;
}
.gzdiv{
  border:1px solid #000;
  padding: 10px;
  background: #fff799;
  position: relative;
  font-family: "微软雅黑";
  color: #49483c;
  margin-top: 2rem;
  padding-top: 1rem;
  margin-bottom: 1rem;
}
.gzdiv span{
  display: inline-block;
  text-align: center;
  width:4rem; 
  height: 1.5rem;
  line-height: 1.5rem;
  border:1px solid #000;
  border-radius: 5px;
  background:#fff799;
  font-size: .8rem; 
  position: absolute;
  top: -.7rem;
  left: 50%;
  margin-left: -2rem;
}
.gzdiv p{
  font-size: .5rem;
  line-height: 1rem;
}
.shopbox{
  height: 2rem;
  width: 100%;
  background: #fff;
  overflow: hidden;
  position: relative;
}
.childiv p{
  float: left;
  width: 50%;
  height: 2rem;
  font-size: .7rem;
  line-height: 2rem;
  overflow: hidden;
  text-align: center;
  color: #8b8b8b;
}
.childiv p.action{
  color: #222222;
  border-bottom: 2px solid #f2201f;
}
.icobox{
  width:1.5rem;
  height: 2rem;
  background: #fff;
  position: absolute;
  right: 0px;
  top: 0px;
  text-align: center;
  line-height: 2rem; 
}
.icobox i{
  display: inline-block;
  width: 1rem;
  height: 1rem;
  background: url(../images/iconext.png) no-repeat;
  background-size: 100% 100%;
}
.swiper-slide {
    text-align: center;
    font-size: 18px;
    background: #fff;
    /* Center slide text vertically */
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: center;
    -ms-flex-pack: center;
    -webkit-justify-content: center;
    justify-content: center;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.infoxa{
  width: 90%;
  margin: 0 auto;
  font-size: .5rem;
  color: #2d2d2d;
}
.infoxb{
  font-size: .8rem;
  color: #e2151c;
  font-weight: bolder;
}
.infoxc{
  font-size: .7rem;
  color: #e2151c;
  font-weight: bolder;
}
.infoxd{
  font-size: .5rem;
  color: #515050;
  display: inline-block;
  margin-left: .5rem;
}
.box-action{
  position: absolute;
  bottom:.5rem;
  right:3px;
  width: 2.6rem;
  height: .7rem;
}
.box-action span{
  width: 1rem;
  display: inline-block;
  height: .7rem;
  line-height: 1.2rem;
  font-size: .6rem;
  position: absolute;
}
.disn,.box-action .disn{
  display: none;
}