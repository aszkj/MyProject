html,
body {
  height: 100%;
  font-size: 26px;
  font-family: "微软雅黑";
  margin: 0 auto;
}
.wd_dva{
  padding:0px 10px;
  background: #fff;
  color: #000;
}
.wd_dva h1{
  font-size: .7rem;
  margin: 5px 0;
  font-weight: bolder;
}
.wd_dva p{
  font-size: .6rem;
  line-height:.8rem; 
  padding:3px 0;
}
.wd_dva h2{
  color: red;
  font-size: .6rem;
  font-weight: bolder;
}
.prolist_ul{
  width: 100%;
  background: #fff;
  height: auto;
  overflow: hidden;
  padding: 0 10px;
}
.prolist_ul li{
  height: auto;
  overflow: hidden;
}

.box_pa{
  padding: 17px 1.2rem 17px 17px;
  border-bottom: 1px solid #dcdcdc;
  font-size: 0.7rem;
  position: relative;
  color:#333;
  background-color: #ffffff;
 }
.box_dva{
  border-bottom: 1px solid #dcdcdc;
  font-size: 0.6rem;
  line-height: 1rem;
  display: none;
  padding: 14px 17px;
  color:#333;
  background-color: #f1f1f1;
}
.box_pa i{
  display: inline-block;
  width: .6rem;
  height: .6rem;
  background: url(../images/ico.png) no-repeat;
  background-size: 100% auto;
  position: absolute;
  right: 17px;
  top: 50%;
  margin-top: -.3rem;
}
.box_pa i.untop{
   margin-top: -.5rem;
  transform: rotate(180deg);
  -webkit-transform: rotate(180deg); 
}
.jd_dva{
  padding-left: 15px;
  position: relative;
}
.jd_dva span{
  position: absolute;
  left: 0px;
  font-size: .6rem;
}
.box_foot{
  position: fixed;
  bottom: 1rem;
  text-align: center;
  width: 100%;
}
.font-sa{
