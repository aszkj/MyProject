}
.bgbox img{
  width: 100%;
  height: auto;
}
.adinfo{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
}
.adinfo img{
   width: 100%;
   height: auto;
   vertical-align: top;
}
