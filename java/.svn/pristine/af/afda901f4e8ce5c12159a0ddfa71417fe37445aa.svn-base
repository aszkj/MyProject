package com.yilidi.o2o.appvo.buyer.product;

import java.util.List;

/**
 * 搜索商品
 * 
 * @author: chenb
 * @date: 2016年5月30日 下午9:03:43
 */
public class SaleProductDetailVO extends SaleProductBaseVO {

    private static final long serialVersionUID = 1078844539581745372L;
    /** 商品详情数据信息 **/
    private String productDetail;
    /**
     * 商品副图
     */
    private List<SaleProductImageListVO> saleProductImageList;
    /**
     * 商品活动信息
     */
    private List<BuyRewardActivityListVO> activityInfoList;

    public String getProductDetail() {
        return productDetail;
    }

    public void setProductDetail(String productDetail) {
        this.productDetail = productDetail;
    }

    public List<SaleProductImageListVO> getSaleProductImageList() {
        return saleProductImageList;
    }

    public void setSaleProductImageList(List<SaleProductImageListVO> saleProductImageList) {
        this.saleProductImageList = saleProductImageList;
    }

    public List<BuyRewardActivityListVO> getActivityInfoList() {
        return activityInfoList;
    }

    public void setActivityInfoList(List<BuyRewardActivityListVO> activityInfoList) {
        this.activityInfoList = activityInfoList;
    }

}
