package com.yilidi.o2o.order.service.dto;

import com.yilidi.o2o.core.model.BaseDto;

/**
 * 生成订单DTO
 * 
 * @author: chenb
 * @date: 2016年6月3日 上午10:59:52
 */
public class SaveOrderDto extends BaseDto {

    private static final long serialVersionUID = 1L;

    /**
     * 订单ID
     */
    private Integer saleOrderId;

    /**
     * 订单编号
     */
    private String saleOrderNo;
    /**
     * 订单已付金额
     */
    private Long paidAmount;
    /**
     * 配送方式编码
     */
    private String deliveryMode;

    /**
     * 配送时间要求
     */
    private String deliveryTimeCode;

    /**
     * 送货时间说明
     */
    private String deliveryTimeNote;
    /**
     * 提货码
     */
    private String receiveCode;

    public Integer getSaleOrderId() {
        return saleOrderId;
    }

    public void setSaleOrderId(Integer saleOrderId) {
        this.saleOrderId = saleOrderId;
    }

    public String getSaleOrderNo() {
        return saleOrderNo;
    }

    public void setSaleOrderNo(String saleOrderNo) {
        this.saleOrderNo = saleOrderNo;
    }

    public Long getPaidAmount() {
        return paidAmount;
    }

    public String getDeliveryMode() {
        return deliveryMode;
    }

    public void setDeliveryMode(String deliveryMode) {
        this.deliveryMode = deliveryMode;
    }

    public void setPaidAmount(Long paidAmount) {
        this.paidAmount = paidAmount;
    }

    public String getDeliveryTimeNote() {
        return deliveryTimeNote;
    }

    public void setDeliveryTimeNote(String deliveryTimeNote) {
        this.deliveryTimeNote = deliveryTimeNote;
    }

    public String getDeliveryTimeCode() {
        return deliveryTimeCode;
    }

    public void setDeliveryTimeCode(String deliveryTimeCode) {
        this.deliveryTimeCode = deliveryTimeCode;
    }

    public String getReceiveCode() {
        return receiveCode;
    }

    public void setReceiveCode(String receiveCode) {
        this.receiveCode = receiveCode;
    }

}
