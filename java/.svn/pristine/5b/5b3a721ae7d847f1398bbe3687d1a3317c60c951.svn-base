package com.yilidi.o2o.product.service.dto;

import com.yilidi.o2o.core.model.BaseDto;

/**
 * 功能描述：产品基础属性值Dto <br/>
 * 作者：zxs <br/>
 * 
 * BugID: <br/>
 * 修改内容： <br/>
 */
public class ProductBasicPropsValueDto extends BaseDto {
	private static final long serialVersionUID = 7254251307861393784L;
	/**
	 * 属性ID
	 */
	private Integer id;
	/**
	 * 产品类别编码, 关联产品域P_PRODUCT_CLASS表的PRODUCTCLASSCODE字段，标识该属性是属于那个类型的产品
	 */
	private String productClassCode;
	/**
	 * 产品ID, 关联产品域产品基础信息表的PRODUCTID字段
	 */
	private Integer productId;
	/**
	 * 属性名称， 与产品域产品扩展属性表P_PRODUCT_BASIC_PROPS中的PROPSNAME字段值一致
	 */
	private String propsName;
	/**
	 * 属性编码，关联产品扩展属性表P_PRODUCT_BASIC_PROPS的属性编码PROPSCODE字段
	 */
	private String propsCode;
	/**
	 * 属性取值，该属性的取值内容
	 */
	private String propsValue;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getProductClassCode() {
		return productClassCode;
	}

	public void setProductClassCode(String productClassCode) {
		this.productClassCode = productClassCode;
	}

	public Integer getProductId() {
		return productId;
	}

	public void setProductId(Integer productId) {
		this.productId = productId;
	}

	public String getPropsName() {
		return propsName;
	}

	public void setPropsName(String propsName) {
		this.propsName = propsName;
	}

	public String getPropsCode() {
		return propsCode;
	}

	public void setPropsCode(String propsCode) {
		this.propsCode = propsCode;
	}

	public String getPropsValue() {
		return propsValue;
	}

	public void setPropsValue(String propsValue) {
		this.propsValue = propsValue;
	}

}