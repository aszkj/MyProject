#import "BaseModel.h"
/**
 *  商品基类模型
 */
@interface GoodsBaseModel : BaseModel

/**
 *  产品id
 */
@property (nonatomic,copy)NSString *productId;
/**
 *  商品id
 */
@property (nonatomic,copy)NSString *goodsId;
/**
 *  商品名称
 */
@property (nonatomic,copy)NSString *goodsName;
/**
 *  商品缩略图
 */
@property (nonatomic,copy)NSString *goodsThumbnail;
/**
 *  商品原价，没有经过任何打折和优惠的价格
 */
@property (nonatomic,strong)NSNumber *goodsOriginalPrice;
/**
 *  商品库存
 */
@property (nonatomic,strong)NSNumber *goodsStock;
/**
 *  商品上下架
 */
@property (nonatomic,strong)NSNumber *goodsOnShelf;
/**
 *  商品描述
 */
@property (nonatomic,copy)NSString *goodsDesc;
/**
 *  商品规格
 */
@property (nonatomic,copy)NSString *goodsStand;

@end
