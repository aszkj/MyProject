#import "PayRequestModel.h"

@implementation PayRequestModel

- (instancetype)initWithOrderNo:(NSString *)orderNo {
    return [self initWithOrderNo:orderNo orderAmount:nil];
}

- (instancetype)initWithOrderNo:(NSString *)orderNo orderAmount:(NSNumber *)orderAmount {

    self = [super init];
    if (self) {
        _orderNo = orderNo;
        _orderAmount = orderAmount;
    }
    return self;
}


@end
