#import "BaseModel.h"

@interface StoreBaseModel : BaseModel
/**
 *  店铺id
 */
@property (nonatomic,copy)NSString *storeId;

/**
 *  店铺名字
 */
@property (nonatomic,copy)NSString *storeName;

/**
 *  店铺电话
 */
@property (nonatomic,copy)NSString *hotline;

/**
 *  店铺评分
 */
@property (nonatomic,assign)CGFloat storeScore;

/**
 *  店铺地址
 */
@property (nonatomic,copy)NSString *storeAdress;

/**
 *  店铺营业开始时间
 */
@property (nonatomic,copy)NSString *storeBusinessBeginTime;

/**
 *  店铺营业结束时间
 */
@property (nonatomic,copy)NSString *storeBusinessEndTime;

/**
 *  店铺满多少免运费
 */
@property (nonatomic,strong)NSNumber *deduceTransCostAmount;

/**
 *  店铺运费
 */
@property (nonatomic,strong)NSNumber *transCostAmount;

/**
 *  店铺状态 暂停营业 正常营业
 */
@property (nonatomic,strong)NSNumber *storeStatus;

/**
 *  店铺所在的市名称
 */
@property (nonatomic,copy)NSString *cityName;



@end
