<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@color/white"
              android:focusable="true"
              android:focusableInTouchMode="true"
              android:orientation="vertical">

    <include layout="@layout/class_action_bar_main"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/brand_listview"
            style="@style/Widget.ListView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingRight="@dimen/padding_normal"
            android:scrollbars="none"/>

        <TextView
            android:id="@+id/dialog"
            android:layout_width="@dimen/button_smaller_width"
            android:layout_height="@dimen/button_small_width"
            android:layout_gravity="center"
            android:background="@mipmap/cart_num_bg"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_largeer"
            android:visibility="gone"/>

        <com.yldbkd.www.seller.android.viewCustomer.SideBar
            android:id="@+id/sidrbar"
            android:layout_width="@dimen/button_small_height"
            android:layout_height="match_parent"
            android:layout_gravity="right|center"
            android:layout_marginBottom="@dimen/brand_classify_margin_big"
            android:layout_marginTop="@dimen/brand_classify_margin_big"
            android:background="@color/white"
            android:visibility="gone"/>
    </FrameLayout>
</LinearLayout>